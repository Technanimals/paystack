{
  "eslint.validate": ["javascript", "javascriptreact"],
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.formatOnSave": false
  },
  "[typescriptreact]": {
    "editor.formatOnSave": false
  },
  "cSpell.words": ["Captcha", "analytics", "flexbox", "grecaptcha"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  }
}
